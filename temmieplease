if getgenv().LegacyVer == true then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/cooldev2926/temmie/refs/heads/main/t3mreal_1-on-yt_temmie-please_heads-pleasescriptlegacy.txt"))()
elseif getgenv().LegacyVer == false then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/cooldev2926/temmie/refs/heads/main/temmieplease.lua"))()
else
    game.Players.LocalPlayer:Kick("Invalid LegacyVer value. Set getgenv().LegacyVer = true or false before executing.")
end
